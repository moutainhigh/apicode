<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.ycandyz.master.dao.mall.goodsManage.MallCategoryDao">
    <resultMap id="BaseResultMap" type="com.ycandyz.master.entities.mall.goodsManage.MallCategory">
        <id column="categoryNo" property="categoryNo" jdbcType="VARCHAR"/>
        <id column="parentCategoryNo" property="parentCategoryNo" jdbcType="VARCHAR"/>
        <id column="categoryName" property="categoryName" jdbcType="VARCHAR"/>
        <id column="categoryImg" property="categoryImg" jdbcType="VARCHAR"/>

    </resultMap>

    <insert id="addMallCategory" parameterType="com.ycandyz.master.entities.mall.goodsManage.MallCategory">
        insert into mall_category(id,shop_no,category_no,parent_category_no,
        category_name,category_img,layer,sort_value,status,created_time,
        updated_time)values (#{id},#{shopNo},#{categoryNo},#{parentCategoryNo},#{categoryName},
        #{categoryImg},#{layer},#{sortValue},#{status},now(),now());
    </insert>

</mapper>


