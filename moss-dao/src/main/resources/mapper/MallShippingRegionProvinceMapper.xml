<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ycandyz.master.dao.mall.goodsManage.MallShippingRegionProvinceDao">
    <resultMap id="BaseResultMap" type="com.ycandyz.master.entities.mall.goodsManage.MallShippingRegionProvince">
        <id column="id" property="id" jdbcType="BIGINT" />
        <id column="region_no" property="regionNo" jdbcType="VARCHAR"/>
        <id column="province" property="province" jdbcType="VARCHAR"/>
        <id column="created_time" property="createdTime" jdbcType="TIMESTAMP" />
        <id column="updated_time" property="updatedTime" jdbcType="TIMESTAMP" />
    </resultMap>
    <delete id="delMallShippingRegionProvinceByshippingNo" parameterType="java.lang.String">
        delete from mall_shipping_region_province where region_no = #{regionNo};
    </delete>

    <select id="selMallShippingRegionProvinceByRegionNo" resultType="java.lang.String" parameterType="java.lang.String">
        select province from mall_shipping_region_province where region_no = #{regionNo};
    </select>

    <insert id="addMallShippingRegionProvince" parameterType="com.ycandyz.master.entities.mall.goodsManage.MallShippingRegionProvince">
        insert into mall_shipping_region_province (region_no,province,created_time,updated_time)
        values (#{regionNo},#{province},now(),now());
    </insert>
</mapper>